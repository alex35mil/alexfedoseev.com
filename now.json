{
  "name": "alexfedoseev.com",
  "version": 2,
  "scope": "alexfedoseev",
  "public": false,
  "builds": [{ "src": "build/**", "use": "@now/static" }],
  "routes": [
    {
      "src": "/(.+)\\.js",
      "dest": "build/$1.js",
      "headers": {
        "Cache-Control": "max-age=31536000",
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.+)\\.css",
      "dest": "build/$1.css",
      "headers": {
        "Cache-Control": "max-age=31536000",
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/(.+)\\.woff",
      "dest": "build/$1.woff",
      "headers": {
        "Cache-Control": "max-age=31536000",
        "Content-Type": "font/woff"
      }
    },
    {
      "src": "/(.+)\\.woff2",
      "dest": "build/$1.woff2",
      "headers": {
        "Cache-Control": "max-age=31536000",
        "Content-Type": "font/woff2"
      }
    },
    {
      "src": "/(.+)\\.jpg",
      "dest": "build/$1.jpg",
      "headers": {
        "Cache-Control": "max-age=31536000",
        "Content-Type": "image/jpeg"
      }
    },
    {
      "src": "/(.+)\\.png",
      "dest": "build/$1.png",
      "headers": {
        "Cache-Control": "max-age=31536000",
        "Content-Type": "image/png"
      }
    },
    {
      "src": "/(.+)\\.gif",
      "dest": "build/$1.gif",
      "headers": {
        "Cache-Control": "max-age=31536000",
        "Content-Type": "image/gif"
      }
    },
    { "src": "/(.+)\\.(.+)", "dest": "build/$1.$2" },
    { "src": "/(.*)", "dest": "build/index.html" }
  ]
}
