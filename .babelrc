{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel",
        "@babel/plugin-syntax-dynamic-import",
        [
          "babel-plugin-prismjs",
          {
            "languages": ["reason", "js", "jsx", "shell", "nginx"],
            "theme": "default",
            "css": true
          }
        ]
      ]
    },
    "production": {
      "plugins": ["@babel/plugin-syntax-dynamic-import"]
    }
  }
}
