{
  "name": "alexfedoseev.com",
  "description": "My personal site",
  "version": "0.0.0",
  "private": true,
  "author": "Alex Fedoseev <alex.fedoseev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "bsb:build": "OCAMLPARAM='_,warn-error=+a' bsb -clean-world -make-world",
    "bsb:watch": "bsb -make-world -w",
    "bsb:clean:watch": "bsb -clean-world -make-world -w",
    "css:generate": "node script/css.js",
    "css:watch": "node script/css.js --watch",
    "photo:generate": "node script/photo.js",
    "photo:watch": "node script/photo.js --watch",
    "webpack:server": "NODE_ENV=development env $(cat .env | xargs) webpack-dev-server --config=config/webpack.config.js",
    "webpack:build": "NODE_ENV=production env $(cat .env | xargs) webpack --config=config/webpack.config.js",
    "webpack:build:preview": "NODE_ENV=production env $(cat .env | xargs) webpack --config=config/webpack.config.js",
    "webpack:build:analyze": "NODE_ENV=production ANALYZE=true env $(cat .env | xargs) webpack --config=config/webpack.config.js",
    "prewebpack:build": "yarn run clean && yarn run css:generate && yarn run photo:generate && yarn run bsb:build",
    "prewebpack:build:preview": "yarn run clean && yarn run css:generate && yarn run photo:generate 11 && yarn run bsb:build",
    "prewebpack:build:analyze": "yarn run clean && yarn run css:generate && yarn run photo:generate 3 && yarn run bsb:build",
    "deploy": "now deploy --target production",
    "deploy:preview": "now deploy --target staging",
    "predeploy": "yarn run webpack:build",
    "predeploy:preview": "yarn run webpack:build:preview",
    "clean": "rm -rf build"
  },
  "dependencies": {
    "bs-nprogress": "5.0.0",
    "bs-webapi": "0.15.2",
    "justified-layout": "3.0.0",
    "nprogress": "0.2.0",
    "photoswipe": "4.1.3",
    "prismjs": "1.17.1",
    "re-classnames": "4.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "reason-react": "0.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/parser": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@mdx-js/loader": "1.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-prismjs": "1.1.0",
    "bs-platform": "5.0.6",
    "compression-webpack-plugin": "3.0.0",
    "core-js": "2.6.5",
    "css-loader": "3.0.0",
    "cssnano": "4.1.10",
    "fast-glob": "3.0.4",
    "fb-watchman": "2.0.0",
    "file-loader": "4.1.0",
    "html-webpack-plugin": "4.0.0-beta.8",
    "image-size": "0.7.4",
    "image-webpack-loader": "5.0.0",
    "linaria": "1.3.1",
    "loader-utils": "1.2.3",
    "lqip": "2.1.0",
    "mini-css-extract-plugin": "0.8.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "1.18.2",
    "react-hot-loader": "4.12.8",
    "robotstxt-webpack-plugin": "6.0.0",
    "sharp": "0.22.1",
    "style-loader": "0.23.1",
    "url-loader": "2.1.0",
    "webapp-webpack-plugin": "2.7.1",
    "webpack": "4.36.1",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.7.2"
  }
}
